# Docker Environment Configuration
# Copy this file to .env.docker and update with your actual values

# Azure OpenAI Configuration (Required)
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_VERSION=2024-12-01-preview
AZURE_OPENAI_DEPLOYMENT_NAME=gpt-4.1

# LangSmith Configuration (Optional but Recommended)
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=supplier-predictor-docker
LANGSMITH_TRACING=true
LANGCHAIN_TRACING_V2=true

# Application Security
SECRET_KEY=your-super-secret-key-for-docker-deployment
API_TOKEN=your-custom-api-token

# Optional: Database URL (SQLite by default)
DATABASE_URL=sqlite:///./supplier_predictor.db

# Optional: Redis Cache Configuration
REDIS_URL=redis://redis:6379/0
